<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :released %><br>
  <%= f.label :released_true, "true" %>
  <%= f.check_box :released %>
  <%= f.label :release_year %>
  <%= f.text_field :release_year %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.submit %>




<% end %>

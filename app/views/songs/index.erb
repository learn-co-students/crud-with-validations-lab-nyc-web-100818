<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Released</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= link_to s.title, song_path(s) %></td>
        <td><%= s.released %></td>
        <td><%= s.release_year %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
        <td><%= link_to 'Edit', edit_song_path(s) %></td>
        <td><%= link_to "Delete", s, method: :delete, data: { confirm: "Really?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Create A Song', new_song_path %>

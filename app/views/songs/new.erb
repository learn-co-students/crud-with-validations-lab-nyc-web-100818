<%= form_for @song do |f| %>
  <%= f.label :title, "Title: " %><br>
  <%= f.text_field :title %><br>
  <%= f.label :released, "Released? " %><br>
  <%= f.check_box :released %><br>
  <%= f.label :release_year, "Release Year: " %><br>
  <%= f.number_field :release_year %><br>
  <%= f.label :artist_name, "Artist: " %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre, "Genre: " %><br>
  <%= f.text_field :genre %><br>
  <%= f.submit %>
<% end %>
